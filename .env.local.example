# WorkOS AuthKit Configuration (Frontend)
VITE_WORKOS_CLIENT_ID=YOUR-WORKOS-CLIENT-ID
VITE_WORKOS_REDIRECT_URI=http://localhost:5173/callback

# Convex Configuration
VITE_CONVEX_URL=your-convex-url

# Demo mode: set to "true" to skip WorkOS auth entirely and render DemoApp.
# Required when auth.config.ts is disabled (convex/auth.config.ts.disabled).
# VITE_DEMO_SKIP_AUTH=true

# Agent server URL (used by ClientDetail to trigger the Python pipeline)
# VITE_AGENT_SERVER_URL=http://localhost:8000
# VITE_AGENT_AUTH_TOKEN=your-token
